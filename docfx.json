{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "**/*.csproj"
                    ]
                }
            ],
            "dest": "api",
            "includePrivateMembers": false,
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "noRestore": false,
            "namespaceLayout": "flattened",
            "memberLayout": "samePage",
            "allowCompilationErrors": false
        }
    ],
    "build": {
        "content": [
            {
                "src": "docfx_project",
                "files": [
                    "**/*.yml",
                    "**/*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "globalMetadata": {
            "_appTitle": "UDPChat",
            "_appName": "UDPChat",
            "_enableSearch": true
        },
        "exportViewModel": true,
        "output": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "modern"
        ],
        "postProcessors": [],
        "keepFileLink": false,
        "disableGitFeatures": false
    },
    "pdf": {
        "content": [
            {
                "src": "docfx_project",
                "files": [
                    "**/*.yml",
                    "**/*.md"
                ]
            }
        ],
        "wkhtmltopdf": {
            "filePath": "/usr/local/bin/wkhtmltopdf",
            "additionalArguments": "--enable-local-file-access"
        },
        "output": "_site_pdf"
    }
}